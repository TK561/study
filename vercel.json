{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/02_ウェブサイト/index.html"
    },
    {
      "src": "/02_ウェブサイト/public/(.*)",
      "dest": "/02_ウェブサイト/public/$1"
    },
    {
      "src": "/main-system",
      "dest": "/02_ウェブサイト/public/main-system/index.html"
    },
    {
      "src": "/main-system/(.*)",
      "dest": "/02_ウェブサイト/public/main-system/$1"
    },
    {
      "src": "/discussion-site",
      "dest": "/02_ウェブサイト/public/discussion-site/index.html"
    },
    {
      "src": "/discussion-site/(.*)",
      "dest": "/02_ウェブサイト/public/discussion-site/$1"
    },
    {
      "src": "/experiment_timeline",
      "dest": "/02_ウェブサイト/public/experiment_timeline/index.html"
    },
    {
      "src": "/experiment_timeline/(.*)",
      "dest": "/02_ウェブサイト/public/experiment_timeline/$1"
    },
    {
      "src": "/esd-framework",
      "dest": "/02_ウェブサイト/public/esd-framework/index.html"
    },
    {
      "src": "/esd-framework/(.*)",
      "dest": "/02_ウェブサイト/public/esd-framework/$1"
    }
  ]
}