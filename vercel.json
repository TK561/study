{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/main-system",
      "dest": "/public/main-system/index.html"
    },
    {
      "src": "/main-system/(.*)",
      "dest": "/public/main-system/$1"
    },
    {
      "src": "/discussion-site",
      "dest": "/public/discussion-site/index.html"
    },
    {
      "src": "/discussion-site/(.*)",
      "dest": "/public/discussion-site/$1"
    },
    {
      "src": "/experiment_results",
      "dest": "/public/experiment_results/index.html"
    },
    {
      "src": "/experiment_results/(.*)",
      "dest": "/public/experiment_results/$1"
    },
    {
      "src": "/enhanced_features",
      "dest": "/public/enhanced_features/index.html"
    },
    {
      "src": "/enhanced_features/(.*)",
      "dest": "/public/enhanced_features/$1"
    },
    {
      "src": "/confidence_feedback",
      "dest": "/public/confidence_feedback/index.html"
    },
    {
      "src": "/confidence_feedback/(.*)",
      "dest": "/public/confidence_feedback/$1"
    }
  ]
}