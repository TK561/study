{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/main-system",
      "dest": "/public/main-system/index.html"
    },
    {
      "src": "/main-system/(.*)",
      "dest": "/public/main-system/$1"
    },
    {
      "src": "/discussion-site",
      "dest": "/public/discussion-site/index.html"
    },
    {
      "src": "/discussion-site/(.*)",
      "dest": "/public/discussion-site/$1"
    },
    {
      "src": "/experiment_timeline",
      "dest": "/public/experiment_timeline/index.html"
    },
    {
      "src": "/experiment_timeline/(.*)",
      "dest": "/public/experiment_timeline/$1"
    }
  ]
}